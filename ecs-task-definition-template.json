{
  "family": "paybyte-merchant-service",
  "networkMode": "awsvpc",  
  "requiresCompatibilities": ["FARGATE"], 
  "executionRoleArn": "arn:aws:iam::476415713484:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::476415713484:role/ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
  "containerDefinitions": [
    {
      "name": "paybyte-merchant-service",
      "image": "476415713484.dkr.ecr.us-east-1.amazonaws.com/test-repo:79dea136f3b8269b3e9913da738d993fee0371be",
      "essential": true,     
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ],
      "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/paybyte-merchant-service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
       }
     }
    }
  ]
}
